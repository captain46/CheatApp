    // Font Awesome because we are awesome
    script(src='https://use.fontawesome.com/110f93fa0e.js')
  body
      header
          h1 #[i.fa.fa-envira(aria-hidden='true')] RIA Cheat Application
          small #[i.fa.fa-behance(aria-hidden='true')] : alpha
      // Navbar
      include navbar
      div#searchLog
      // Add Snippet Dialog
      include addDialog
      article#offStor
          #myOfflineStorage
      block content
    include footer
